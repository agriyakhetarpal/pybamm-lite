 	( cd SuiteSparse_config && $(MAKE) both )
-	( cd Mongoose && $(MAKE) both )
 	( cd AMD && $(MAKE) both )
 	( cd BTF && $(MAKE) both )
-	( cd CAMD && $(MAKE) both )
-	( cd CCOLAMD && $(MAKE) both )
