-	( cd GPUQREngine && $(MAKE) global )
-	( cd SPQR && $(MAKE) global )
-	( cd GraphBLAS && $(MAKE) global )
-	( cd SPEX && $(MAKE) global )
 
 # compile; "sudo make install" will install only in /usr/local
 # (or whatever your CMAKE_INSTALL_PREFIX is)
